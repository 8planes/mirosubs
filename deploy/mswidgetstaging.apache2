<VirtualHost *:80>
   ServerAdmin adam@8planes.com

   ServerName mswidgetstaging.8planes.com
   ServerAlias mswidgetstaging.8planes.com

   LogLevel warn
   ErrorLog /home/mirosubsstaging/log/apache.widget.error.log
   CustomLog /home/mirosubsstaging/log/apache.widget.custom.log combined

   Alias /media /var/www/
   <Location "/media">
      SetHandler None
      Order allow,deny
      Allow from all
   </Location>

   ScriptAlias / /home/mirosubsstaging/mirosubs/widget-demo/
</VirtualHost>
