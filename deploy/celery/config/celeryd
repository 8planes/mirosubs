# Where the Django project is.
# PROJECT_ROOT is where settings.py exists
CELERYD_CHDIR="<PROJECT_ROOT>/"

# path to virtual enviroment
VIRTUALENV="<ENV_PATH>"

# Name of the projects settings module.
export DJANGO_SETTINGS_MODULE="settings"

CELERYD="<PROJECT_ROOT>/manage.py"
CELERYEV="<PROJECT_ROOT>/manage.py"
CELERYBEAT="<PROJECT_ROOT>/manage.py"

#celeryd options
CELERYD_OPTS="celeryd -E --concurrency=10 -n worker1.dev"

#celeryevcam options
CELERYEV_OPTS="celeryev --frequency=5.0"
CELERYEV_CAM="djcelery.snapshot.Camera"

#celerybeat options
CELERYBEAT_OPTS="celerybeat"